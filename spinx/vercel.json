{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/api/assets/(.*)",
      "dest": "/api/assets/$1"
    },
    {
      "src": "/([^/.]+)",
      "dest": "/api/pages/$1.php"
    },
    {
      "src": "/",
      "dest": "/api/index.php"
    }
  ]
}
