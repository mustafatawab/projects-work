{
  "version": 2,
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.7.4"
    }
  },
  "routes": [
    {
      "src": "/([^/.]+\\.php)",
      "dest": "/$1"
    },
    {
      "src": "/([^/.]+)",
      "dest": "/$1.php"
    },
    {
      "src": "/",
      "dest": "/index.php"
    }
  ]
}
